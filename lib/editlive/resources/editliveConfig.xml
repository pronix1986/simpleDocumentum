    <splashScreen shown="false"/>
    <menuBar showAboutMenu="false" />
    <wysiwygEditor tabPlacement="bottom" showDocumentNavigator="false"/>
    <sourceEditor enabled="true" showBodyOnly="true" errorDialog="simple" />

    <htmlFilter indentContent="false" encloseText="true"
        logicalEmphasis="false" outputXHTML="true" outputXML="true"
        quoteMarks="false" removeFontTags="false" allowUnkownTags="true"
        uppercaseAttributes="false" uppercaseTags="false" wrapLength="0"/>
