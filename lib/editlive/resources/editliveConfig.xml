    <mediaSettings>
        <httpUpload base="" href=""/>
        <images allowLocalImages="true" allowUserSpecified="true">
            <imageList/>
        </images>
    </mediaSettings>
    <hyperlinks>
        <hyperlinkList/>
        <mailtoList/>
    </hyperlinks>
    <menubar showAboutMenu="false"/>
